{
    "name": "hyperf/hyperf-skeleton",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperf",
        "microservice",
        "middleware"
    ],
    "description": "A coroutine framework that focuses on hyperspeed and flexible, specifically use for build microservices and middlewares.",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.4",
        "ext-bcmath": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-swoole": ">=4.6",
        "ext-zip": "*",
        "codedungeon/php-cli-colors": "^1.12",
        "hyperf/amqp": "2.1.*",
        "hyperf/async-queue": "2.1.*",
        "hyperf/cache": "2.1.*",
        "hyperf/command": "2.1.*",
        "hyperf/config": "2.1.*",
        "hyperf/constants": "2.1.*",
        "hyperf/consul": "2.1.*",
        "hyperf/database": "2.1.*",
        "hyperf/db-connection": "2.1.*",
        "hyperf/elasticsearch": "2.1.*",
        "hyperf/engine": "^1.0",
        "hyperf/filesystem": "2.1.*",
        "hyperf/framework": "2.1.*",
        "hyperf/guzzle": "2.1.*",
        "hyperf/http-server": "2.1.*",
        "hyperf/json-rpc": "2.1.*",
        "hyperf/logger": "2.1.*",
        "hyperf/memory": "2.1.*",
        "hyperf/model-cache": "2.1.*",
        "hyperf/nsq": "2.1.*",
        "hyperf/process": "2.1.*",
        "hyperf/rate-limit": "2.1.*",
        "hyperf/redis": "2.1.*",
        "hyperf/rpc": "2.1.*",
        "hyperf/rpc-client": "2.1.*",
        "hyperf/rpc-server": "2.1.*",
        "hyperf/service-governance": "2.1.*",
        "hyperf/socketio-server": "2.1.*",
        "hyperf/task": "2.1.*",
        "hyperf/tracer": "2.1.*",
        "hyperf/view": "2.1.*",
        "hyperf/view-engine": "2.1.*",
        "hyperf/websocket-client": "2.1.*",
        "hyperf/websocket-server": "2.1.*",
        "limingxinleo/hyperf-utils": "^3.0",
        "overtrue/flysystem-qiniu": "^1.0",
        "phper666/jwt-auth": "^3.0",
        "phpmailer/phpmailer": "^6.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "swoole/ide-helper": "^4.5",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "hyperf/devtool": "2.1.*",
        "hyperf/testing": "2.1.*",
        "symfony/var-dumper": "^5.1"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Kernel/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "./test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": [],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "rm -rf runtime/container"
        ],
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./app ./config",
        "start": "php ./bin/hyperf.php start"
    }
}
